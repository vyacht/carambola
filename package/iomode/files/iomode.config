config gpio_mode
	option uartf 'UARTF_GPIO'
	option uartl 'UARTL'
	option spi   'SPI'
	option i2c   'GPIO'

config gpio 'GPIO11'
	option export  11
	option value 1
	option direction out
	
config dev 'serial0'
	option device '/dev/ttyS0'
	option speed '4800'

config dev 'serial1'
	option device '/dev/ttyS1'
	option speed '4800'

